{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": ["error", "windows"],
    "max-len": ["error", { "code": 120 }],
    "no-underscore-dangle": "off"
  }
} 